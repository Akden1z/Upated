game.StarterGui:SetCore("SendNotification", {
    Title = "Made By.";
    Text = "! Kmari#7856"; -- (Cr: ! Kmari#7856)
    Duration = 10;
})

local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wall%20v3')))()

local w = library:CreateWindow("Main") -- Creates the window

local b = w:CreateFolder("SlientAim") -- Creates the folder(U will put here your buttons,etc)
b:Label("Thanks for using ! Kmari#7856",{
    TextSize = 13; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
    
}) 

b:Button("SlientAim",function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Akden1z/SlientAim/main/CounterBlox', true))()
end)
b:Button("Unlock All skins",function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Akden1z/Allskins/main/EpicUssles", true))()
end)
-------
b:Toggle("Toggle SlientAim",function(bool)
    shared.toggle = bool
    print(shared.toggle)
end)
b:Toggle("ANTI-LAG",function(bool)
    shared.toggle = bool
    print(shared.toggle)
end)
b:Toggle("No depencies",function(bool)
    shared.toggle = bool
    print(shared.toggle)
end)
b:Toggle("Fix Cam always",function(bool)
    shared.toggle = bool
    print(shared.toggle)
end)
